{
  "name": "express-custom",
  "version": "1.0.2",
  "description": "Easily define and document custom express routes and endpoints.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "bin": {
    "gensite": "./dist/bin/bin/gensite.js",
    "export-api": "./dist/bin/bin/export-api.js"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts --ext .ts && tsc",
    "lint:fix": "eslint --fix src/**/*.ts --ext .ts && tsc",
    "copy:site": "copyfiles -u 1 --exclude site/node_modules/** --exclude site/out/** --exclude site/.next/** site/**/* dist/site",
    "copy": "npm run copy:site",
    "build:bin": "rimraf dist/bin && tsc --build tsconfig.bin.json",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts --minify --clean --sourcemap && npm run copy && npm run build:bin",
    "docs": "rimraf docs && typedoc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slekup/express-custom"
  },
  "keywords": [
    "express",
    "custom",
    "docs",
    "documentation",
    "api",
    "nodejs"
  ],
  "author": "Slekup",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slekup/express-custom/issues"
  },
  "homepage": "https://github.com/slekup/express-custom#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "highlight.js": "^11.8.0",
    "logform": "^2.5.1",
    "mongoose": "^7.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@changesets/cli": "^2.26.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-prettier": "^0.0.0-empty",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.7",
    "typedoc-plugin-extras": "^2.3.3",
    "typedoc-plugin-mdn-links": "^3.0.3"
  },
  "files": [
    "dist"
  ]
}
